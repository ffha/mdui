{
  "name": "mdui",
  "version": "1.0.2",
  "description": "a CSS Framework based on material design",
  "main": "dist/js/mdui.js",
  "module": "dist/js/mdui.esm.js",
  "types": "es/index.d.ts",
  "style": "dist/css/mdui.css",
  "less": "src/index.less",
  "scripts": {
    "eslint": "eslint --fix src/**/*.ts",
    "stylelint": "stylelint --fix src/**/*.less",
    "lint": "npm run eslint && npm run stylelint",
    "js": "npm run eslint && tsc -p src/tsconfig.json && rollup --config",
    "css": "npm run stylelint && gulp",
    "build": "npm run js && npm run css",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "files": [
    "dist",
    "es",
    "src",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zdhxiong/mdui.git"
  },
  "keywords": [
    "material design",
    "responsive",
    "framework",
    "front-end",
    "mobile-first",
    "web"
  ],
  "author": "zdhxiong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zdhxiong/mdui/issues",
    "email": "zdhxiong@gmail.com"
  },
  "homepage": "https://mdui.org",
  "dependencies": {
    "mdn-polyfills": "^5.20.0",
    "mdui.jq": "^2.0.1",
    "normalize.css": "^8.0.1",
    "promise-polyfill": "^8.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.3.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "autoprefixer": "^10.2.4",
    "core-js": "^3.27.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-header": "^2.0.9",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^5.0.9",
    "pinst": "^2.1.4",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "rollup": "^3.9.1",
    "stylelint": "^13.10.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "tslib": "^2.1.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14"
  }
}
